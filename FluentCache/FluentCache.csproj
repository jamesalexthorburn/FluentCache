<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.1</TargetFramework>
    <Version>4.0.0.2</Version>
    <Authors>Evan Rash</Authors>
    <Company />
    <PackageTags>Cache Fluent</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>FluentCache is a simple, fluent library to help you write clean, legible caching code by reducing boilerplate.</Description>
    <AssemblyVersion>4.0.0.2</AssemblyVersion>
    <FileVersion>4.0.0.2</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <WarningLevel>4</WarningLevel>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <WarningsAsErrors />
    <NoWarn />
    <DocumentationFile>bin\Debug\netstandard1.2\FluentCache.xml</DocumentationFile>
  </PropertyGroup>

</Project>
